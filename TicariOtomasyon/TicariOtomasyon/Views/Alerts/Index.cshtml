
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}

<script src="https://unpkg.com/sweetalert/dist/sweetalert.min.js"></script>
<script src="~/Scripts/jquery-3.7.1.js"></script>

<br />
<br />
<button class="btn btn-primary" id="btn1">Button1</button>
<button class="btn btn-warning" id="btn2">Button2</button>
<button class="btn btn-info" id="btn3">Button3</button>
<button class="btn btn-danger" id="btn4">Button4</button>
<button class="btn btn-default" style="background-color:aquamarine" id="btn5">Button5</button>

<script>
    $('#btn1').click(function () {
        swal("Hello world!");
    });
</script>

<script>
    $('#btn2').click(function () {
        swal("Here's the title!", "...and here's the text!");
    });
</script>

<script>
    $('#btn3').click(function () {
        swal("Good job!", "You clicked the button!", "success");
    });
</script>

<script>
    $('#btn4').click(function () {
        swal({
            title: "Good job!",
            text: "You clicked the button!",
            icon: "success",
        });
    });
</script>

<script>
    $('#btn5').click(function () {
        swal({
            title: "Good job!",
            text: "You clicked the button!",
            icon: "success",
            button: "Aww yiss!",
        });
    });
</script>